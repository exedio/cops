<?xml version="1.0"?>
<project
		xmlns:ivy="antlib:org.apache.ivy.ant"
		name="ivy"
		default="resolve">
	<ivy:settings file="ivysettings.xml" />
	<target name="resolve">
		<delete dir="build/ivy" />
		<mkdir dir="build/ivy" />
		<ivy:retrieve pattern="build/ivy/[artifact]-[revision]-[type].[ext]" />
	</target>
</project>
